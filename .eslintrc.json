{
  "root": true,
  "parser": "@typescript-eslint/parse",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": [
    "next/core-web-vitals",
    "next/typescript",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": ["warn", { "extension": [".ts", ".tsx"] }],
    "no-unused-vars": "off"
  }
}
